  HEADER_TYPE        = "FITS"                                                 
  BYTES              = 2880                                                   
  RECORDS            = 1                                                      
  INTERCHANGE_FORMAT = "BINARY"                                               
  DESCRIPTION        = "Minimal FITS header, added by SBN personnel"          
